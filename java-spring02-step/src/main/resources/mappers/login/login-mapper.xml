<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<!-- java Resource/src/main/resources/mappers/member/member-mapper.xml		 -->
<!--  다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="login">
	
	<select id="login_check" resultType="String">
		select username from tbl_member
		where userid = #{userid} and userpw = #{userpw }
	</select>	
	
	<select id="viewLogin" resultType="com.example.spring02.login.model.dto.LoginVO">
		select * 
		from tbl_member
		where userid = #{userid} and userpw = #{userpw }
	</select>	
	
	

	
</mapper>